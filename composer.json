{
    "name": "reneknox/fetch",
    "description": "wrap for curl request",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.20"
    },
    "autoload": {
        "psr-4": {
            "Reneknox\\Fetch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Reneknox\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "php vendor/bin/phpunit"
    },
    "authors": [
